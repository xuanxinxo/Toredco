generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"  
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String
  createdAt DateTime @default(now())
}
